            "stopTimeout": 60,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/ReceiptsBE-Task1-Definition1",
                    "awslogs-region": "me-south-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
